{
  "name": "injectable-ts",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/raveclassic/injectable-ts",
  "workspaces": [
    "./packages/*"
  ],
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "scripts": {
    "build": "nx run-many --target=build --all --parallel",
    "lint": "nx run-many --target=lint --all --parallel",
    "test": "nx run-many --target=test --all --parallel",
    "preversion": "npm i --frozen-lockfile",
    "version": "npm i && git add package-lock.json",
    "postversion": "git push",
    "predeploy": "npm run build",
    "deploy": "nx run-many --target=deploy --all",
    "docs": "npm run docs --prefix packages/core && npm run docs --prefix packages/react"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-angular": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nrwl/cli": "13.10.1",
    "@nrwl/eslint-plugin-nx": "13.10.1",
    "@nrwl/jest": "13.10.1",
    "@nrwl/js": "13.10.1",
    "@nrwl/linter": "13.10.1",
    "@nrwl/workspace": "13.10.1",
    "@types/jest": "~27.4.1",
    "@types/node": "~16.11.26",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "eslint": "~8.13.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-expect-type": "~0.1.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~26.1.4",
    "eslint-plugin-unicorn": "~42.0.0",
    "husky": "^7.0.4",
    "jest": "~27.5.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "lerna": "^4.0.0",
    "ngx-deploy-npm": "^3.1.7",
    "nx": "13.10.1",
    "prettier": "~2.6.2",
    "ts-jest": "~27.1.4",
    "typescript": "~4.6.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
